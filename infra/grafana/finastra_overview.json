{
  "__inputs": [],
  "__requires": [
    {"type": "panel", "id": "graph", "name": "Graph"},
    {"type": "datasource", "id": "prometheus", "name": "Prometheus"}
  ],
  "title": "Finastra Overview",
  "templating": {
    "list": [
      {
        "name": "env",
        "type": "query",
        "datasource": "Prometheus",
        "query": "label_values(finastra_api_calls_total, env)",
        "includeAll": true
      },
      {
        "name": "service",
        "type": "query",
        "datasource": "Prometheus",
        "query": "label_values(finastra_api_calls_total, service)",
        "includeAll": true
      }
    ]
  },
  "panels": [
    {
      "type": "graph",
      "title": "Error rate (5m)",
      "targets": [
        {
          "expr": "sum(rate(finastra_api_calls_total{status!~\"2..\", env=~\"$env\", service=~\"$service\"}[5m])) / ignoring(status) sum(rate(finastra_api_calls_total{env=~\"$env\", service=~\"$service\"}[5m]))"
        }
      ]
    },
    {
      "type": "graph",
      "title": "P95 latency (5m)",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (le, endpoint) (rate(finastra_api_latency_seconds_bucket{env=~\"$env\", service=~\"$service\"}[5m])))"
        }
      ]
    },
    {
      "type": "graph",
      "title": "Breaker state (max over 5m)",
      "targets": [
        {
          "expr": "max_over_time(finastra_breaker_state{client=\"collateral\", env=~\"$env\", service=~\"$service\"}[5m])"
        }
      ]
    }
  ],
  "schemaVersion": 36,
  "version": 1
}
